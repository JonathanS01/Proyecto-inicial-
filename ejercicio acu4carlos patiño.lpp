entero numero, suma 
inicio 
	numero <-1
	suma <-0
		mientras (numero <= 10) haga
			escriba suma
			llamar nueva_linea
			suma <-suma + numero 
			numero <-numero + 1 
				llamar obtener_caracter
				escriba numero, "  "
		fin mientras
			escriba "La suma total es", suma 
			
			
			fin